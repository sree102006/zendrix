pipeline {
  agent any
  stages{
     stage('Init') {
        steps {
           echo " This is from stage one Init stage "
        }

     }
     stage('Build') {
        steps {
           echo "This is Build stage"
        }
     }
     stage('Deploy'){
        steps{
           echo "Thi is Deploying Stage"  
        }

     }


  }


}
